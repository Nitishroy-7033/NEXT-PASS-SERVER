{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "MongodbConfigs": {
    "ConnectionString": "${MONGODB_CONNECTION_STRING}",
    "DatabaseName": "${MONGODB_DATABASE_NAME}"
  },
  "Jwt": {
    "Subject": "${JWT_SUBJECT}",
    "Issuer": "${JWT_ISSUER}",
    "Audience": "${JWT_AUDIENCE}",
    "Key": "${JWT_SECRET_KEY}",
    "ExpireMinutes": 60
  },
  "MongoDB": {
    "PublicApiKey": "${MONGODB_PUBLIC_API_KEY}",
    "PrivateApiKey": "${MONGODB_PRIVATE_API_KEY}",
    "ProjectId": "${MONGODB_PROJECT_ID}"
  },
  "AllowedHosts": "your-domain.com"
}
